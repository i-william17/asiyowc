{
  "expo": {
    "name": "asiyowc",
    "slug": "asiyowc",
    "scheme": "asiyowc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/asiyo-nobg.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/asiyo-nobg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.asiyowc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/asiyo-nobg.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.asiyowc",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/images/asiyo-nobg.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "react-native-compressor",
      "react-native-video",
      "expo-video",
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bc56a75f-6b8a-4619-8118-6199e886c21b"
      }
    },
    "owner": "williamwritescode"
  }
}
